@startuml

partition 指揮者 {
  (*) --> "ステージへあがる"
  --> === Sf ===
  --> "お辞儀をする" as bow
}

partition 聴衆 LightSkyBlue
  === Sf === --> "拍手"
end partition

partition 指揮者 {
  bow --> === Sj ===
  --> "タクトを振る" as wave
  "拍手" --> === Sj ===
}

partition オーケストラ #CCCCEE {
  wave -> "準備"
  --> "演奏する"
}
@enduml
